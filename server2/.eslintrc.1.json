{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "linebreak-style": ["error", "unix"],
        "quotes": ["error", "double"],
        "semi": ["error", "always"],
        "no-var": "error",
        "prefer-const": ["error", { "ignoreReadBeforeAssign": false }]
    },
    "plugins": ["prettier"]
}
